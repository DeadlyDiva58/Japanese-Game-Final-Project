.node-label
{
	color: #000000;
	height: 32px;
	width: 100%;
	font-size: 18px;
	-unity-text-align: middle-center;
	margin-top: 6px;
	flex-shrink: 0;
	flex-direction: column;
	margin-bottom: auto;
	padding-bottom: 12px;
	-unity-font-style: bold;
	/*-unity-font: resource('Fonts/Protocol');
	-unity-font-definition: none;*/
}

.node 
{
	-unity-background-image-tint-color: #FF0000;
	-unity-slice-scale: 0.5px;
	background-image: resource('images/components/node_box_light_transparent');
	background-color: rgba(0,0,0,0);
	width: 400px;
	height: 200px;
	position: absolute;
	align-items: center;
	align-content: flex-start;
	justify-content: center;
	flex-direction: column;
	flex-grow: 1;
	padding-bottom: 0px;
}

.node-input-panel
{
	background-color: rgba(0,0,0,0);
	align-items: flex-start;
	flex-direction: column;
	flex-grow: 0;
	flex-shrink: 0;
	justify-content: center;
	position: relative;
	margin-left: 4px;
	padding-top: 1px;
	padding-bottom: 1px;
	width: 24px;
	height: auto;
}

.node-input 
{
	background-image: resource('images/ports/node_port');
	position: relative;
	background-color: rgba(0,0,0,0);
	width: 20px;
	height: 20px;
	margin-bottom: 2px;
	margin-top: 2px;
	justify-content: center;
	-unity-background-image-tint-color: #00FF00;
}

.node-output-panel
{
	background-color: rgba(0,0,0,0);
	align-items: flex-end;
	flex-direction: column;
	flex-grow: 0;
	flex-shrink: 0;
	justify-content: center;
	position: relative;
	margin-right: 4px;
	padding-top: 1px;
	padding-bottom: 1px;
	width: 24px;
	height: auto;
}

.node-output 
{
	background-image: resource('images/ports/node_port');
