.vertical-resize-handle 
{
	cursor: resize-vertical;
	opacity: 0;
}

.horizontal-resize-handle 
{
	cursor: resize-horizontal;
	opacity: 0;
}

.top-resize-handle
{
	width: 60%;
	height: 6px;
	position: absolute;
	top: 0px;
	align-self: center;
	margin-bottom: auto;
	margin-top: auto;
}

.bottom-resize-handle
{
	width: 60%;
	height: 6px;
	position: absolute;
	bottom: 0px;
	align-self: center;
	margin-bottom: auto;
	margin-top: auto;
}

.left-resize-handle
{
	width: 6px;
	height: 60%;
	position: absolute;
	align-self: flex-start;
}

.right-resize-handle
{
	width: 6px;
	height: 60%;
	position: absolute;
	align-self: flex-end;
}

